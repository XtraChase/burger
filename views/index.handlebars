<h1>Burger</h1>

<h2>Eat Burgers</h2>
{{#each burger}}
<p>this is a burger</p>
{{/each}}

<h2>Add a burger</h2>
<form class="create-form">

    <div class="form-group">
        <label for="ca">Burger Name:</label>
        <input type="text" id="burger-text-input" name="name">
    </div>

    <button type="submit" id="submit">Add Burger</button>
</form>